@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />

    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="../../Scripts/Views/User/User_EditPassWord.js"></script>
</head>
<body style="text-align:center">
    <table style="text-align:center" align="center">
        <tr>
            <td style="width: 80px;">原始密码：</td>
            <td>
                <input class="easyui-textbox" id="orign_password" onblur="orign_passwordBlur()" required="required">
            </td>
            <td style="width:100px;">
                <label><span id="Info_ORG" style=" color:red; "></span></label>
            </td>
        </tr>
        <tr>
            <td>新密码：</td>
            <td>
                <input class="easyui-textbox" id="newPass" required="required">
            </td>
            <td>
                <label><span id="Info_NEW" style=" color:red; "></span></label>
            </td>
        </tr>
        <tr>
            <td>确认密码：</td>
            <td>
                <input class="easyui-textbox" id="passwordConfirm" onblur="recheckPWD()" required="required">
            </td>
            <td>
                <label><span id="Info_Confirm" style=" color:red; "></span></label>
            </td>
        </tr>
    </table>
    <div style="text-align: center">
        <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add'" onclick="submitNewPWD()">确认</a>
        <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-cancle'" onclick="cancleSubmt()">取消</a>
    </div>
</body>
</html>
