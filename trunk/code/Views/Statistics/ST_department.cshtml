@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />

    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>

    <script type="text/javascript" src="../../Scripts/Views/Statistics/ST_department.js"></script>
</head>
<body>
    <div class="easyui-panel" fit="true" style="padding: 10px">
            <select class="easyui-combobox" name="state" style="width: 80px;" id="dateType">
                <option value="GZRQ" selected="selected">购置日期</option>
                <option value="DJRQ">登记日期</option>
            </select>
            开始时间：
                        <input class="easyui-datebox" id="beginDate_SC" style="width: 100px;" data-options="sharedCalendar:'#cc',formatter:myformatter,parser:myparser,editable:false">
            结束时间:
                        <input class="easyui-datebox" id="endDate_SC" style="width: 100px;" data-options="sharedCalendar:'#cc',formatter:myformatter,parser:myparser,editable:false">
            资产类别：
              <select class="easyui-combotree" id="AssetType_ST" name="AssetType_ST" style="width: 200px;">
              </select>
            <a href="#" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-reload'" onclick="clearAssetType()"></a>
            <div style="float: right; text-align: right"> 
                <input type="radio" name="table_TYPE" onclick="chanegeTableType_Radio()" value="1" checked="checked" />明细
                <input type="radio" name="table_TYPE" onclick="chanegeTableType_Radio()" value="0" />汇总
            </div>
            <br />
            供应商：
             <select class="easyui-combogrid" id="supplier_ST" name="supplier_ST" style="width: 200px;">
             </select>
            <a href="#" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-reload'" onclick="clearSupplier();"></a>
            资产性质：
             <select class="easyui-combobox" id="ATAttr_ST" name="ATAttr_ST" style="width: 200px;">
             </select>
            <a href="#" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-tip'" onclick="doSearch()">统计</a>
            <a href="#" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-reload'" onclick="clearALL()">重置</a>
            <br />
            <table id="datagrid_ST_DE" class="easyui-datagrid" style="height: 600px" data-options="border:false,singleSelect:true,fitColumns:true" fit="true"></table>

       
    </div>
</body>
</html>
